filetype off
set smartcase
set hidden
set nocompatible
syntax on
set hls
set is
set ic
set hls
set textwidth=79
set tabstop=4
set shiftwidth=4
set expandtab
set autoindent
set encoding=utf-8
set rtp+=/etc/vim/bundle/Vundle.vim
call vundle#begin()
Plugin 'VundleVim/Vundle.vim'
Plugin 'elzr/vim-json'
call vundle#end()
filetype plugin indent on
